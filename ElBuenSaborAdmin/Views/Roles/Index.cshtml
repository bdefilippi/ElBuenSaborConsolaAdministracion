@model IEnumerable<ElBuenSaborAdmin.Models.Rol>

@{
    ViewData["Title"] = "Roles";
}

<h1>Roles</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>

            </td>
        </tr>
}
    </tbody>
</table>
